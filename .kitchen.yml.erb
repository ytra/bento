---
provisioner:
  name: <%= @provisioner %>

platforms:
- name: "<%= @boxname %>"
  driver:
    name: vagrant
    synced_folders:
      - [".", "/vagrant", "disabled: <%= @share_disabled %>"]
    provider: <%= @provider %>
    box: bento-<%= @boxname %>
    box_url: <%= @box_url %>

suites:
- name: default
